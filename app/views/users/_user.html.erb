
<div class="row span7 text-center users page" id="users page">
  <div class="col-md-12"><h2></h2></div>
       
       <div class="index-background users page" id="users page">
          <div class="col-md-12"><h3>Name: <%= user.name %></h3></div>
          <div class="col-md-12">Email: <%= user.email %></div>
          <div class="col-md-12">Linkedin Profile: <%= user.url %></div>
          <div class="col-md-12">Country: <%= user.country %></div>
          <div class="col-md-12">Location: <%= user.location %></div>
          <div class="col-md-12">Current Latutiude: <%= user.latitude %></div>
          <div class="col-md-12">Current Longitude: <%= user.longitude %></div>
          <div class="col-md-12">Description: <%= user.description %></div>
          <div class="col-md-12"><%= link_to "Contact this Professional", user_path(user.id) %></div>

           <br />
          <div><img src="<%= user.image_url %>" width="100" /></div>
         </div>
  </div>
<br />


  