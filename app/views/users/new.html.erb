<% if current_user %>

	<%= form_for @user do |f| %>

	<div>
	    <%= f.label :address %>
	    <%= f.text_field :address %>
	</div>
	<div>
	    <%= f.label :city %>
	    <%= f.text_field :city %>
	</div>
	<div>
	    <%= f.label :zipcode %>
	    <%= f.text_field :zipcode %>
	</div>
	<div>
	    <%= f.label :state %>
	    <%= f.text_field :state %>
	</div>

	<%= f.submit "Add new user" %>

	<% end %>

<% else %> 

	<% link_to root_path %>

<%end%>